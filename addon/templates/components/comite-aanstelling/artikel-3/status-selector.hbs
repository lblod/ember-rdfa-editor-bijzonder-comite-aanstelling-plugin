{{#if statusCodes}}
  {{#power-select
     options=statusCodes
     allowClear=true
     selected=mandataris.status
     onchange=(action "select")
     renderInPlace=true
     as |status|}}
    {{#if status.label}}
      {{status.label}}
    {{else}}
      {{status.value}}
    {{/if}}
  {{/power-select}}
{{else}}
  Aan het laden...
{{/if}}
